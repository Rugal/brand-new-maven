type Mutation {
  addCourse(input: CourseInput!): Course!
  addStudent(input: StudentInput!): Student!
  addRegistration(input: RegistrationInput!): Registration!
}

input CourseInput {
  name: String!
}

input StudentInput {
  name: String!
}

input RegistrationInput {
  grade: Int
  sid: Int!
  cid: Int!
}
